<url address="http://admin-apps.webofknowledge.com/JCR/JCR?SID" bulkRecords="300" crawledSites="120">
	<link type="text" value="establish a new session" />

	<form name="limits" buttonName="Submit" as="first">
		<edition options="JCR Science Edition;JCR Social Sciences Edition"/>
		<science_year>JCR Science Edition Year</science_year>
		<social_year value="2009">JCR Social Sciences Edition Year</social_year>
		<RQ options="View a group of journals by;Search for a specific journal;View all journals"/>
		<view>View a group of journals by options:</view>
	</form>
	<if first.RQ="View a group of journals by">
		<form no="1" as="form1">
			<query_data>Select one or more categories from the list.</query_data>
			<RQ> Select to view Journal data or aggregate Category data.</RQ>
			<journal_sort_by/>
			<category_sort_by/>
		</form>
		<if form1.RQ="LIST_SUMMARY_JOURNAL">
			<content as="abbreviated">
				<text>Abbreviated Journal Title</text>
				<link type="text" path="parent/4.sibling/child/2.sibling/child" linkType="text" next="2.parent/sibling/child/2.sibling/child" terminator="input" terminatorType="name" terminatorPath="2.parent/sibling"/>
				<nextPageLink type="img" value="Next Page"/>
			</content>
		</if>
		<else>
			<content as="category">
				<text>Category</text>
				<link type="text" path="parent/4.sibling/child/2.sibling/child" linkType="text" next="2.parent/sibling/child/2.sibling/child" terminator="input" terminatorType="name" terminatorPath="2.parent/sibling"/>
				<nextPageLink type="img" value="Next Page"/>
			</content>
		</else>
	</if>
	<else>
		<if first.RQ="Search for a specific journal">
			<form no="1">
				<guery_type/>
				<query_data/>
			</form>
			<content ref="abbreviated"/>
		</if>
		<else>
			<if first.RQ="View all journals">
				<content ref="abbreviated"/>
			</if>
		</else>
	</else>
	
	<form no="1" as="form1">
		<query_data>Select one or more categories from the list.</query_data>
		<RQ> Select to view Journal data or aggregate Category data.</RQ>
		<journal_sort_by/>
		<category_sort_by/>
	</form>
	
	
